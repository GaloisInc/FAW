/* This parser comes from: https://web.archive.org/web/20161124014949/https://www.sdms.afrl.af.mil/index.php?collection=tools_nitf */
{
  // This appears to be a GUI-only program at the moment.
  disabled: true,

  parsers: {
    nitf: {
      exec: ['/usr/bin/env', 'PYTHONPATH=.', 'python3', 'main.py', '<inputFile>'],
      version: '1',
      parse: {
        type: 'regex-counter',
        version: '1',
        stdstar: {
          '.*': {
            nameReplace: {
              '[0-9]+': '<INT>',
            },
          },
        },
      },
    },
  },

  build: {
    stages: {
      final: {
        commands: [
          'RUN pip3 install kaitaistruct',
        ],
      },
    },
  },
}
