{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "outDir": "dist",
    "strict": true,
    "jsx": "preserve",
    //"importHelpers": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "baseUrl": ".",
    "lib": [
      "esnext"
    ],
    "paths": {
      "@/*": [
        "common/*"
      ]
    }
  },
  "include": [
    "*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
