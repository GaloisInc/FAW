{
    parsers: {
        "pdfium": {
            disabled: true,
            exec: ['/opt/pdfium/pdfium_test', '<inputFile>'],
            version: 'chromium-4180',
            parse: {
                type: 'regex-counter',
                version: '1',
                stdstar: {
                    '^\\s*(/opt|/lib|[#]FailureMessage).*$': {
                        nameGroup: '<<workbench: stack trace collapsed>>'
                    },

                    '^.*$': {
                        nameReplace: {'[0-9]': ''},
                    },
                },
            },
        },
    },

