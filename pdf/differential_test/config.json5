{
  file_detail_views: {
    differential: {
        label: 'Visual Differential Comparison',
        type: 'program_to_html',
        exec: ['./main.py', '<artifactInParserDirs pageImages100dpi>', '<tempDir>', '--html'],
    },
  },
  parsers: {
    'differential-test': {
      exec: ['./main.py', '<artifactInParserDirs pageImages100dpi>', '<tempDir>'],
      timeout: 120,
      version: 'differential-test72',
      parse: {
        type: 'regex-counter',
        version: '2',
        stdstar: {
          '^(Max RMSE, .*?): (.*?)$': {
            nameGroup: 1,
            countGroup: 2,
            countAsNumber: true,
          },
          '^(Max diff, .*?): (.*?)$': {
            nameGroup: 1,
            countGroup: 2,
            countAsNumber: true,
          },
          '^Differential: .*$': {
            nameReplace: {
              '[0-9]': '',
            },
          },
          '^(subprocess\.CalledProcessError)': {
            nameGroup: 1,
          },
          '^Page ': {
            nameGroup: '',
          },
          '^(Traceback|  )': {
            nameGroup: '<<workbench: collapsed errors>>',
            nameReplace: {
              '[0-9]': '',
            },
          },
        },
      },
    },
  },

  build: {
    stages: {
      final: {
        commands: [
          'RUN apt-get update && apt-get install -y imagemagick',
          'COPY {dist}/requirements.txt {disttarg}/requirements.txt',
          'RUN pip3 install --upgrade pip \
            && pip3 install -r {disttarg}/requirements.txt',
        ],
      },
    },
  },
}

